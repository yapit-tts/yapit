[
  {
    "name": "Skip URLs",
    "description": "Remove raw http/https URLs.",
    "config": {
      "regex_rules": [
        {
          "pattern": "\\bhttps?://\\S+",
          "replacement": ""
        }
      ],
      "llm": {} 
    }
  },
  {
    "name": "Skip text inside parentheses",
    "description": "Delete any text inside ().",
    "config": {
      "regex_rules": [
        {
          "pattern": "\\([^)]*\\)",
          "replacement": ""
        }
      ],
      "llm": {}
    }
  },
  {
    "name": "Skip text inside brackets",
    "description": "Delete any text inside [].",
    "config": {
      "regex_rules": [
        {
          "pattern": "\\[[^]]*\\]",
          "replacement": ""
        }
      ],
      "llm": {}
    }
  },
  {
    "name": "Skip text inside braces",
    "description": "Delete any text inside {}.",
    "config": {
      "regex_rules": [
        {
          "pattern": "\\{[^}]*\\}",
          "replacement": ""
        }
      ],
      "llm": {}
    }
  }
]
